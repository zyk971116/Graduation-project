<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jf.school.mapper.MoneyMapper">
    <select id="query" resultType="com.jf.school.bean.table.Money">
        select * from money;
    </select>
    <update id="update">
        UPDATE money set our_money=#{ourMoney},other_money=#{otherMoney}
    </update>
</mapper>
